@model EndpointMvc.Models.EndpointData

<div class="">
	<ul class="nav nav-list endpoint-nav-bar">
		@foreach ( var item in Model.Areas ) {
			<li>
				<i class="icon-chevron-right"></i>
				<a href="#@(item.QualifiedName.Replace(".","_"))">@item.Name</a>
				<ul class="nav nav-list endpoint-nav-bar endpoint-nav-sub-bar l2">
					@foreach ( var secitem in item.ServicesList ) {
						<li>
							<i class="icon-chevron-right"></i>
							<a href="#@(secitem.QualifiedName.Replace(".","_"))">@secitem.Name</a>
								<ul class="nav nav-list endpoint-nav-bar endpoint-nav-sub-bar l3">
									@foreach ( var epitem in secitem.EndpointList ) {
										<li>
											<i class="icon-chevron-right"></i>
											<a href="#@(epitem.QualifiedName.Replace(".","_"))">@epitem.Name</a>
										</li>
									}
								</ul>
						</li>
					}
				</ul>
			</li> 
		}
	</ul>

	<div class="row mvl">
		<script type="text/javascript">
			google_ad_client = "ca-pub-9157593394596390";
			google_ad_slot = "3339975035";
			google_ad_width = 300;
			google_ad_height = 600;
		</script>
		<script type="text/javascript" src="http://pagead2.googlesyndication.com/pagead/show_ads.js"></script>
	</div>
</div>